IDW_MAIN MENU 
BEGIN
    POPUP "&File"
    BEGIN
        MENUITEM "New Files Tab",               IDM_NEW_FILES
        MENUITEM "New Classes Tab",             IDM_NEW_CLASSES
		MENUITEM "New Dialog Tab",              IDM_NEW_DIALOG
        MENUITEM "Close Tab",                   IDM_CLOSE_TAB
        MENUITEM SEPARATOR
        MENUITEM "E&xit",                       IDM_FILE_EXIT
    END
    POPUP "&Edit"
    BEGIN
        MENUITEM "Undo\tCtrl+Z",                IDM_EDIT_UNDO, GRAYED
        MENUITEM "Redo\tShift+Ctrl+Z",          IDM_EDIT_REDO, GRAYED
        MENUITEM SEPARATOR
        MENUITEM "Cut\tCtrl+X",                 IDM_EDIT_CUT, GRAYED
        MENUITEM "Copy\tCtrl+C",                IDM_EDIT_COPY, GRAYED
        MENUITEM "Paste\tCtrl+V",               IDM_EDIT_PASTE, GRAYED
        MENUITEM "Delete\tDel",                 IDM_EDIT_DELETE, GRAYED
    END
    POPUP "&View"
    BEGIN
        MENUITEM "&Tool Bar",                   IDW_VIEW_TOOLBAR, CHECKED
        MENUITEM "&Status Bar",                 IDW_VIEW_STATUSBAR, CHECKED
    END
    POPUP "&Options"
    BEGIN
        MENUITEM "Owner Draw",                  IDM_TAB_DRAW, CHECKED
		MENUITEM "Fixed Tab Width",             IDM_TAB_FIXED, CHECKED
		MENUITEM SEPARATOR
        MENUITEM "Tabs at Top",                 IDM_TAB_TOP, CHECKED
        MENUITEM "Show Buttons",                IDM_TAB_BUTTONS, CHECKED
    END
    POPUP "&Help"
    BEGIN
        MENUITEM "&About",                      IDM_HELP_ABOUT
    END
END

IDM_NEWMENU MENU 
