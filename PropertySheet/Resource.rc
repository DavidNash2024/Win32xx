    BEGIN
        MENUITEM "Modeless Property Sheet",     IDM_MODELESS
        MENUITEM "Modal Property Sheet",        IDM_MODAL
        MENUITEM "Wizard",                      IDM_WIZARD
    END
    POPUP "&Help"
    BEGIN
        MENUITEM "&About",                      IDM_HELP_ABOUT
    END
END


